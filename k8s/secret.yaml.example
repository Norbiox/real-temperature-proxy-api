# Example Secret configuration for Open-Meteo API key
# Copy this file to secret.yaml and update with your actual API key
# DO NOT commit secret.yaml to version control!

apiVersion: v1
kind: Secret
metadata:
  name: openmeteo-secrets
  labels:
    app: real-temperature-proxy-api
type: Opaque
stringData:
  # Replace with your actual Open-Meteo API key
  api-key: "your-api-key-here"
